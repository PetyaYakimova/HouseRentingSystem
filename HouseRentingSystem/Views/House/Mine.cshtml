@model IEnumerable<HouseServiceModel>
@{
	ViewBag.Title = "My Houses";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any() == false)
{
	<h3 class="text-center">You have no hauses yet!</h3>
}
else
{
	<div class="row">
		@foreach (var item in Model)
		{
			<partial name="_HousePartial" model="@item"></partial>
		}
	</div>
}